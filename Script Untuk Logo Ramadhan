Desktop
<script>
    window.onload = function() {
        // Mendapatkan semua elemen <img> di halaman
        var images = document.querySelectorAll('img');
        
        // Loop untuk memeriksa setiap gambar
        images.forEach(function(img) {
            // Cek apakah src mengandung '/assets/img/alt/logo.png'
            if (img.src.includes('/assets/img/alt/logo.png')) {
                // Mengubah src menjadi URL baru
                img.src = "https://takenupload.com/67bec1035ebb9";
            }
        });
    };
</script>
